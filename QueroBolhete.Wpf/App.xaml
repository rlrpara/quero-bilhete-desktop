             xmlns:local="clr-namespace:QueroBolhete.Wpf"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Bootstraper x:Key="Bootstraper" />
